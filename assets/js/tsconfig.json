{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2018",
    "noEmit": true,
    "lib": ["dom", "dom.iterable", "es2018"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "typeRoots": [
      "../../node_modules/@types",
      "./@types",
    ]
  },
  "exclude": [],
  "include": ["**/*.ts"],
}